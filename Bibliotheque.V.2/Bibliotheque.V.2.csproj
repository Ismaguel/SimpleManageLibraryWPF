<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Bibliotheque.V._2</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="LookUp\IconData.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>IconData.resx</DependentUpon>
    </Compile>
    <Compile Update="LookUp\LangString.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LangString.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="LookUp\IconData.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>IconData.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="LookUp\LangString.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>LangString.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
